{
     "version": 2,
     "builds": [
       {
         "src": "index.html",
         "use": "@vercel/static"
       },
       {
         "src": "public/**",
         "use": "@vercel/static"
       },
       {
         "src": "src/**",
         "use": "@vercel/static"
       }
     ],
     "routes": [
       {
         "src": "/src/(.*)",
         "dest": "/src/$1"
       },
       {
         "src": "/public/(.*)",
         "dest": "/public/$1"
       },
       {
         "src": "/assets/(.*)",
         "dest": "/public/assets/$1"
       },
       {
         "src": "/styles.css",
         "dest": "/public/styles.css"
       },
       {
         "src": "/(.*)",
         "dest": "/index.html"
       }
     ]
   }
